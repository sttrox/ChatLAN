<Page x:Class="ChatLAN.Pages.Client"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ChatLAN.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Client">

    <StackPanel>
        <TextBlock>Send</TextBlock>
        <TextBox Name="TbSend">Send</TextBox>
        <Button Click="ButtonBase_OnClick"> Send</Button>
        <TextBlock Margin="20">Get</TextBlock>
        <TextBox Name="TbGet">Send</TextBox>
    </StackPanel>
</Page>
